{
	"name": "dkuzeit",
	"private": true,
	"version": "1.0.0",
	"license": "MIT",
	"author": "Roman Kurakin",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"preview": "vite build && wrangler dev",
		"build": "svelte-kit sync && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.check.json && eslint .",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.check.json --watch",
		"fixtures:sync": "npx tsx scripts/download-fixtures.ts",
		"fixtures:parse": "npx tsx scripts/parse-fixtures.ts",
		"test": "vitest run",
		"test:e2e": "playwright test",
		"format": "prettier --write .",
		"deploy": "npm run build && wrangler deploy"
	},
	"dependencies": {
		"@evilmartians/harmony": "^1.4.0",
		"@lucide/svelte": "^0.563.1",
		"@sentry/sveltekit": "^10.38.0",
		"bits-ui": "^2.15.5",
		"runed": "^0.37.1",
		"ts-ics": "^2.4.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@inlang/paraglide-js": "^2.10.0",
		"@inlang/plugin-m-function-matcher": "^2.2.0",
		"@inlang/plugin-message-format": "^4.2.0",
		"@playwright/test": "^1.58.2",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.2.3",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"html-rewriter-wasm": "^0.4.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.50.1",
		"svelte-check": "^4.3.6",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.18",
		"wrangler": "^4.64.0"
	}
}
